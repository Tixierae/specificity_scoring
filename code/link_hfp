                <!-- DFP PREFIX CONFIGURATION -->
<script type="text/javascript">
    window.dfpPrefix = "";
    window.dfpPrefixController = {};
    (function(){
        var slotPrefix = "/128139881/HUFF_huffpost/home/home/";

        
            
                //For FR, MG
                
                    slotPrefix = slotPrefix + "hp/";
                
            
        

        
            
        
        
        
            
        
        

        var results = new RegExp('[\?&]dfp_test_prefix=([^&#]*)').exec(window.location.href);
        if (results && results[1]){
            window.dfpPrefix = results[1].trim().replace(/^\/+|\/+$/g, '');
        } else {
            window.dfpPrefix = slotPrefix;
        }
    })();
</script>
<!-- HEAD CODE -->
<script type="text/javascript">
    window.dfpController = (function() {
        var dfpDivToSlotMap = {};
        
        var dfpController = {
            console: {
                error: function(m){ var ll = window.console && window.console.error(m); },
                debug: function(m){ var ll = window.console && window.console.debug(m); },
                log: function(m){ var ll = window.console && window.console.log(m); }
            },
            methods: {
                waitForGlobal: function(identifier, cb, checkInterval) {
                    var test = typeof identifier === "function" ? identifier : function() { return !!window[identifier]; };
                    var interval;
                    if (test()) {
                        cb();
                    } else {
                        interval = setInterval(function(){
                            if (test()) { cb(); clearInterval(interval); }
                        }, checkInterval || 50);
                    }
                },
                randomNumber: function() {
                    return Math.round(Math.random()*1e16);
                },
                registerDfpSlotDiv: function(slot_name){
                    if (window.dfpSlotIds &&
                            "string" === typeof slot_name &&
                            "undefined" === typeof window.dfpSlotIds[slot_name]) {
                        window.dfpSlotIds[slot_name] = "ready";
                    }
                    return;
                },
                registerSlot: function(div_id, slot) {
                    var _tmp = !dfpDivToSlotMap[div_id] && (dfpDivToSlotMap[div_id] = slot);
                },
                getSizesByPlacementSlug: function(placement_slug){
                    if (window.dfpSizes && window.dfpSizes[placement_slug]) {
                        return window.dfpSizes[placement_slug];
                    } else {
                        dfpController.console.error("Sizes not defined for placement: " + placement_slug);
                        return [];
                    }
                },
                getDfpSlotDivs: function(){
                    return window.dfpSlotIds || {};
                },
                attachAdReloadEvent: function(dfp_div_el, dfp_slot){
                    try {
                        // 'jQuery' is a hard dependency
                        dfpController.methods.waitForGlobal("jQuery", function(){
                            jQuery(document).on('reloadAd', function(ev, elem) {
                                if (!elem.contains(dfp_div_el)) {
                                    return;
                                }
                                dfpController.console.debug("Refresh Ad inside: #" + dfp_div_el.id);
                                googletag.pubads().refresh([dfp_slot]);
                                return;
                            });
                        });
                    } catch (e) {
                        dfpController.console.error(e.message);
                    }
                }
            }
        };

        var initDfpSizes = function(){
            
            window.dfpSizes = {
                "leaderboard_flex": [[1000, 200], [1000, 90], [970, 250], [970, 90], [728, 90]],
                "leaderboard_below_nav": [[1000, 200], [1000, 90], [970, 250], [970, 90], [728, 90]],
                "leaderboard_bottom": [[1000, 200], [1000, 90], [970, 250], [970, 90], [728, 90]],
                "slideshow_728x90_req": [[970, 90], [970, 250], [728, 90]],
                "slideshow_300x250_req": [300, 250],
                "right_rail_flex": [[300, 600], [300, 250], [300, 1050], [300, 800]],
                "right_rail_lower": [[300, 250]],
                "right_rail_300x250_lower": [[300, 250]],
                "entry_paragraph_4": [300, 250],
                "sharebox_260x60": [260, 60],
                "blog_rail_top_flex": [[300, 250]],
                "text_sponsorship": [300,250]
            };
            
            
            return;
        };

        var initDfpSlots = function(){
            window.dfpSlotIds = {};
            return;
        };

        var initDfpGlobals = function(){
            window.googletag = window.googletag || {};
            window.googletag.cmd = window.googletag.cmd || [];
            window.googletag.slots = window.googletag.slots || {};
        };

        var includeScriptAsync = function(src, protocol){
            try {
                protocol = protocol || 'https:' == document.location.protocol ? "https:" : "http:";
                var srcHasProtocol = src.match(/htt(p|ps):\/\//) && src.match(/htt(p|ps):\/\//).length;
                if (srcHasProtocol) {
                    protocol = "";
                }
                var node = document.getElementsByTagName('script')[0];
                var scr = document.createElement('script');
                scr.async = true;
                scr.type = 'text/javascript';
                scr.src = protocol + src;
                node.parentNode.insertBefore(scr, node);
            } catch (e) {
                dfpController.console.error(e.message);
            }
            return;
        };

        var initDfpTargeting = function(){
            var targeting = {
                rub: [],
                keywords: []
            };
            
            
            targeting.rub.push("homepage");
            
            
            var navtype = 0;
            if (typeof performance !== "undefined" && typeof performance.navigation !== "undefined" ) {
                navtype = performance.navigation.type;
            } else {
                if (document.referrer === window.location.href) navtype = 1;
            }
            // add a custom targeting to DFP call to pass the navigation type
            targeting.nt = "" + navtype;
            
            //temp
            function get_url_query(variable) {
                var query = window.location.search.substring(1);
                var vars = query.split('&');
                for (var i = 0; i < vars.length; i++) {
                    var pair = vars[i].split('=');
                    if (decodeURIComponent(pair[0]) == variable) {
                        return decodeURIComponent(pair[1]);
                    }
                }
                return '';
            }
            var dfptest = get_url_query('dfptest');
            if (dfptest !== '') {
                targeting.test = dfptest;
            }

            googletag.cmd.push(function() {
                googletag.pubads().enableAsyncRendering();
                googletag.pubads().collapseEmptyDivs();
                googletag.pubads().enableSingleRequest();
                for (var k in targeting) {
                    googletag.pubads().setTargeting(k, targeting[k]);
                }
                googletag.enableServices();
                try {nvg41021.dfpnvg();}catch(e){}
            });
        };
        
        var initDfpRenderEvent = function() {
            var rendered_divs = [];
            googletag.cmd.push(function(){
                googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                    for(var div_id in dfpDivToSlotMap) {
                        var slot = dfpDivToSlotMap[div_id];
                        if (event.slot == slot) {
                            rendered_divs.push(div_id);
                        }
                    }
                    dfpController.methods.waitForGlobal('jQuery', function(){
                        (function($){
                            for(var j = 0; j < rendered_divs.length; j++){
                                $("#" + rendered_divs[j]).addClass("ad-rendered");
                            }
                        })(jQuery);
                    });
                    return;
                });
            });
        };

        // Synchronous
        initDfpSizes();
        initDfpSlots();
        initDfpGlobals();
        includeScriptAsync('//www.googletagservices.com/tag/js/gpt.js');
        initDfpTargeting();
        initDfpRenderEvent();

        return dfpController;
    })();
</script>
<!-- HEAD CODE ENDS -->
<script>
window.waitForGlobal = function(identifier, cb, poll) {
  var test = typeof identifier === "function" ? identifier : function() { return !!window[identifier]; };
  var interval;
  if(test()) {
    cb();
  } else {
    interval = setInterval(function(){
      if(test()) {
        cb();
        clearInterval(interval);
      }
    }, poll || 50);
  }
};

window.modulousQueue = (function() {
  var queue = [];
  var run = false;
  function runner(fn) {
    setTimeout(function(){
      try {
        fn();
      } catch (e) {
      }
    }, 0);
  }
  return {
     add: function(fn) {
       !run && queue.push(fn);
       run && runner(fn);
     },
     runAll: function() {
       var i = 0;
       run = true;
       for(;i<queue.length;i++) {
         runner(queue[i]);
       }
       queue = [];
     }
  };
}());

setTimeout(function(){
  waitForGlobal('jQuery', modulousQueue.runAll);
}, 5000);
</script>
<script>
var _HPJS = (function (_mod) {
  "use strict";

  _mod.loadJS = function (src, options) {
    var options = options || {};
    var ref = window.document.getElementsByTagName("script")[0];
    var script = window.document.createElement("script");
    script.src = src;
    script.async = options.async;
    script.defer = options.defer;
    ref.parentNode.insertBefore(script, ref);
    return script;
  }

  _mod.loadJSLater = function (url, timeout) {
    var ran = false;
    var loader = function () {
      if (ran) {
        return;
      }
      ran = true;
      _mod.loadJS(url, {async:true});
    }
    window.addEventListener ?
      window.addEventListener("load",loader,false) :
      window.attachEvent && window.attachEvent("onload",loader);
    // in case the load event never fires
    setTimeout(loader, timeout || 15000);
  }

  _mod.waitForGlobal = function(identifier, cb, poll) {
    var test = typeof identifier === "function" ? identifier : function() { return !!window[identifier]; };
    var interval;
    if(test()) {
      cb();
    } else {
      interval = setInterval(function(){
        if(test()) {
          cb();
          clearInterval(interval);
        }
      }, poll || 50);
    }
  };

  return _mod;

}(_HPJS || {}));
</script>

