        <div id="modulous_top_1x1" class="ad_spot">
            <!-- TEMPLATE CODE -->
<div id="dfp_habillage" class="dfp clearfix ad-top_1x1">
<script type="text/javascript">
    (function(){
        var dfpController = window.dfpController;

        var dfp_placement_slug = 'top_1x1';
        var dfp_slot_id = 'habillage';
        var dfp_div_id = "dfp_" + dfp_slot_id;
        var dfp_div_el = document.getElementById(dfp_div_id);

        

        var dfp_prefix = window.dfpPrefix || "";
        if (dfp_prefix === "") dfpController.console.error("DFP prefix is empty");
        dfp_prefix = dfp_prefix + dfp_slot_id;
      
        //DEBUGGING
        //dfpController.console.log(dfp_placement_slug + ": " + dfp_prefix);

        var execute_display_in_template = false;
        

        googletag.cmd.push(function(){
            var dfp_slot = 
                googletag.defineOutOfPageSlot(dfp_prefix, dfp_div_id)
                    .setTargeting('pos', dfp_slot_id)
                    .addService(googletag.pubads());

            if (execute_display_in_template) {
                googletag.cmd.push(function(){ googletag.display(dfp_div_id); });
            } else {
                dfpController.methods.registerDfpSlotDiv(dfp_div_id);
            }
            
            dfpController.methods.registerSlot(dfp_div_id, dfp_slot);

            dfpController.methods.attachAdReloadEvent(dfp_div_el, dfp_slot);
            return;
        });

        return;
    })();
    </script>
</div>
<!-- TEMPLATE CODE ENDS --><!-- TEMPLATE CODE -->
<div id="dfp_dhtml" class="dfp clearfix ad-top_1x1">
<script type="text/javascript">
    (function(){
        var dfpController = window.dfpController;

        var dfp_placement_slug = 'top_1x1';
        var dfp_slot_id = 'dhtml';
        var dfp_div_id = "dfp_" + dfp_slot_id;
        var dfp_div_el = document.getElementById(dfp_div_id);

        

        var dfp_prefix = window.dfpPrefix || "";
        if (dfp_prefix === "") dfpController.console.error("DFP prefix is empty");
        dfp_prefix = dfp_prefix + dfp_slot_id;
      
        //DEBUGGING
        //dfpController.console.log(dfp_placement_slug + ": " + dfp_prefix);

        var execute_display_in_template = false;
        

        googletag.cmd.push(function(){
            var dfp_slot = 
                googletag.defineOutOfPageSlot(dfp_prefix, dfp_div_id)
                    .setTargeting('pos', dfp_slot_id)
                    .addService(googletag.pubads());

            if (execute_display_in_template) {
                googletag.cmd.push(function(){ googletag.display(dfp_div_id); });
            } else {
                dfpController.methods.registerDfpSlotDiv(dfp_div_id);
            }
            
            dfpController.methods.registerSlot(dfp_div_id, dfp_slot);

            dfpController.methods.attachAdReloadEvent(dfp_div_el, dfp_slot);
            return;
        });

        return;
    })();
    </script>
</div>
<!-- TEMPLATE CODE ENDS --><!-- TEMPLATE CODE -->
<div id="dfp_parallaxe" class="dfp clearfix ad-top_1x1">
<script type="text/javascript">
    (function(){
        var dfpController = window.dfpController;

        var dfp_placement_slug = 'top_1x1';
        var dfp_slot_id = 'parallaxe';
        var dfp_div_id = "dfp_" + dfp_slot_id;
        var dfp_div_el = document.getElementById(dfp_div_id);

        

        var dfp_prefix = window.dfpPrefix || "";
        if (dfp_prefix === "") dfpController.console.error("DFP prefix is empty");
        dfp_prefix = dfp_prefix + dfp_slot_id;
      
        //DEBUGGING
        //dfpController.console.log(dfp_placement_slug + ": " + dfp_prefix);

        var execute_display_in_template = false;
        

        googletag.cmd.push(function(){
            var dfp_slot = 
                googletag.defineOutOfPageSlot(dfp_prefix, dfp_div_id)
                    .setTargeting('pos', dfp_slot_id)
                    .addService(googletag.pubads());

            if (execute_display_in_template) {
                googletag.cmd.push(function(){ googletag.display(dfp_div_id); });
            } else {
                dfpController.methods.registerDfpSlotDiv(dfp_div_id);
            }
            
            dfpController.methods.registerSlot(dfp_div_id, dfp_slot);

            dfpController.methods.attachAdReloadEvent(dfp_div_el, dfp_slot);
            return;
        });

        return;
    })();
    </script>
</div>
<!-- TEMPLATE CODE ENDS -->
        </div>
    
            <div id="AolTrafficInfo" data-blogid="1045"></div>
    
